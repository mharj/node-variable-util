    "devDependencies": {
        "@types/chai": "^4.3.1",
        "@types/chai-as-promised": "^7.1.5",
        "@types/mocha": "^9.1.1",
        "@types/node": "^16.11.36",
        "@types/sinon": "^10.0.11",
        "chai": "^4.3.6",
        "chai-as-promised": "^7.1.1",
        "mocha": "^10.0.0",
        "sinon": "^14.0.0",
        "ts-node": "^10.8.0",
        "typescript": "^4.7.2"
    },
    "dependencies": {
        "dotenv": "^16.0.1"
