{
    "name": "mharj-node-variable-util",
    "version": "0.0.8",
    "description": "Utility to get variable from multiple resources",
    "main": "./dist/index.js",
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run test && npm run build",
        "test": "mocha"
    },
    "mocha": {
        "exit": true,
        "extension": [
            "ts",
            "js"
        ],
        "recursive": true,
        "require": [
            "ts-node/register",
            "source-map-support/register"
        ],
        "reporters": [
