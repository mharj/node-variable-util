  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.32",
    "@types/sinon": "^9.0.11",
    "chai": "^4.3.3",
    "mocha": "^8.3.1",
    "sinon": "^9.2.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
