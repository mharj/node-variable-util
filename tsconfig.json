{
    "compilerOptions": {
        "lib": [
            "es2020"
        ],
        "module": "commonjs",
        "target": "es2020",
        "rootDir": "./src",
        "outDir": "./dist",
        "moduleResolution": "node",
        "sourceRoot": "./src",
        "types": [
            "node"
        ],
        "declaration": true,
        "removeComments": false,
        "inlineSources": false,
        "inlineSourceMap": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "importHelpers": false,
        "strictNullChecks": true,
        "suppressImplicitAnyIndexErrors": true,
        "noUnusedLocals": true,
    }
}